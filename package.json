{
  "name": "spiegelmatch-core",
  "version": "1.0.0",
  "description": "SpiegelMatch - Avatar-based anonymous kink dating with 8D character generation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:frontend": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "build": "tsc && vite build",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "start": "node dist/server.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src frontend/src --ext .ts,.tsx",
    "lint:fix": "eslint src frontend/src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\" \"frontend/src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\" \"frontend/src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "demo": "tsx scripts/demo.ts",
    "clean": "rm -rf dist node_modules frontend/dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^10.16.15",
    "helmet": "^8.1.0",
    "lucide-react": "^0.263.1",
    "morgan": "^1.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^7.2.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
